{ "manifest_version": 2

, "name": "Cyberscore Notifications"
, "description": "Keep up to date with your notifications with this handy extension."
, "version": "0.1.0"

, "permissions": [ "alarms"
                 , "background"
                 , "notifications"
                 , "http://api.dev/"
                 , "http://cs-api.herokuapp.com/"
                 ]

, "browser_action": { "default_icon": "icon.png"
                    , "default_popup": "popup.html"
                    }

, "options_page": "options.html"
}
